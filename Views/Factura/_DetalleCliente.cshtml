@model MedicalDentCRM.Models.DetalleClienteModelo

<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLongTitle">Detalles del cliente</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

<div class="modal-body">
    <div class="aliceblue-bg p-4 clearfix">
        <div class="pull-left right-space">
            <div style="border-radius: 37px;padding: 10px;box-shadow: #1c1d530d 1px 6px 10px 2px;" class="white-bg">
                <svg id="Layer_1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" class="icon icon-xxlg-sm text-top"><switch><g><path fill="#4077FF" d="M421.3 446.3H90.7C40.9 446.3.6 406 .6 356.2V155.8c0-49.8 40.3-90.1 90.1-90.1h330.7c49.8 0 90.1 40.3 90.1 90.1v200.4c-.1 49.7-40.4 90.1-90.2 90.1z"></path><circle fill="#FFF" cx="138.3" cy="213.6" r="43.4"></circle><path fill="#FFF" d="M203.5 350.4H73.1c-12.6 0-21-13.1-15.5-24.4 14.4-30.1 45.1-50.9 80.7-50.9s66.3 20.8 80.7 50.9c5.4 11.3-3 24.4-15.5 24.4zm60.7-162.9h192v26.2h-192zm0 90.6h111.2v26.2H264.2z"></path></g></switch></svg>
            </div>
        </div> <div class="pull-left">
            <div class="bottom-space"></div>
            <div>
                <strong class="font-xlarge">@Model.NombreCliente</strong>
                <a href="javascript:void(0)" onclick="detalleCliente(this)" data-clienteid="@Model.ClienteId" class="ember-view">
                    <svg id="Layer_1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" class="icon icon-xs"><path d="M432 319.3c-17.7 0-32 14.3-32 32v64.3c0 15.3-19.7 32.3-48 32.3l-240 .1c-12.7 0-24.7-5-33.9-14.1C69 424.8 64 412.7 64 400V160c0-26 16.4-48 35.9-48h67.9c17.7 0 32-14.3 32-32s-14.3-32-32-32H99.9C44.8 48 0 98.2 0 160v240c0 29.8 11.7 57.9 32.9 79.1C54.1 500.3 82.2 512 112 512l240-.1c61.8 0 112-43.2 112-96.3v-64.3c0-17.7-14.3-32-32-32z"></path><path d="M512 52c0-28.7-23.3-52-52-52H344c-17.7 0-32 14.3-32 32s14.3 32 32 32h54.4L202.3 261.5c-12.5 12.5-12.4 32.8.2 45.3 6.2 6.2 14.4 9.3 22.5 9.3 8.2 0 16.5-3.2 22.7-9.5L448 104.9v63.3c0 17.7 14.3 32 32 32s32-14.3 32-32V52z"></path></svg>
                </a>
            </div>
        </div>
    </div>
    <div class="p-3">
        <div class="row">
            <div class="col-md-7">
                <div class="text-muted" style="font-size:14px;">Importes pendientes de cobro</div>
                <div data-test-title="ar-amount" class="font-xxlarge top-small-space">@string.Format("{0}{1}", Model.CodigoMoneda, Model.ImportePendiente.FormatearADecimales())</div>
            </div> <div class="col-md-5 text-right">
                <div class="text-muted" style="font-size:14px;">Créditos sin usar</div>
                <div class="font-xxlarge top-small-space">@string.Format("{0}{1}", Model.CodigoMoneda, Model.CreditoSinUsar.FormatearADecimales())</div>
            </div>
        </div> <div id="ember5030" class="accord-title-arrow top-space ember-view">
            <a href="#collapseExample" data-toggle="collapse" role="button" aria-expanded="false" id="ember5031" class="accord-title collapsed ember-view">
                Otros detalles
                <span class="caret"></span>
            </a>
            <div id="collapseExample" class="ember-view collapse" style="height: 0px;font-size:14px;">
                <div class="row top-space">
                    <label class="col-md-5 text-muted"> Cód. de moneda </label>
                    <div class="col-md-7"> @Model.CodigoMoneda </div>
                </div><div class="row top-small-space">
                    <label class="col-md-5 text-muted"> Estado del portal </label>
                    <div class="col-md-7"><span class="text-muted"> @Model.EstadoPortalStr </span></div>
                </div> <div class="row top-small-space">
                    <label class="col-md-5 text-muted"> Idioma del portal </label>
                    <div data-test-title="other-details-language" class="col-md-7"> @Model.Idioma </div>
                </div>
            </div>
        </div>
    </div>
    <div class="tabDetalleCliente">
        <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">DIRECCIÓN</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">PERSONAS DE CONTACTO</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">ACTIVIDAD</a>
            </li>
        </ul>
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                <div id="address-tab" class="tab-pane top-space active">
                    <div class="popovercontainer address-group ember-view">
                        <span class="d-inline-block left-right-space align-top">
                            <svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" x="0" y="0" viewBox="0 0 512 512" xml:space="preserve" class="icon text-success "><path d="M432 512c-4.3 0-8.4-1.7-11.5-4.9l-49.2-50.8-38.7 49.5c-2.8 3.6-7.1 5.8-11.6 6.1-4.6.3-9-1.4-12.3-4.7l-52.6-52.6-51.7 52.5c-3.2 3.3-7.7 5-12.3 4.7-4.6-.3-8.9-2.5-11.7-6.1l-38.8-49.6-50.2 51c-4.6 4.6-11.5 6-17.5 3.6-6-2.5-9.9-8.3-9.9-14.8V32C64 14.4 78.4 0 96 0h320c17.6 0 32 14.4 32 32v464c0 6.5-4 12.4-10 14.8-1.9.8-4 1.2-6 1.2zm-176-96c4.2 0 8.3 1.7 11.3 4.7l44.8 44.8c3.4 3.4 9 3.1 12-.7l33.3-42.6c2.8-3.6 7.1-5.9 11.8-6.1 4.6-.2 9.1 1.5 12.3 4.8l31.1 32.1c1.3 1.3 3.4.4 3.4-1.4V48c0-8.8-7.2-16-16-16H112c-8.8 0-16 7.2-16 16v404.1c0 1.8 2.2 2.7 3.4 1.4l32.2-32.7c3.2-3.3 7.7-5 12.3-4.7s8.9 2.5 11.7 6.1l33.2 42.5c3 3.8 8.6 4.1 12 .7l43.8-44.5c3-3.2 7.1-4.9 11.4-4.9z"></path><path d="M255 145.2c0 8.7-7.1 15.8-15.8 15.8h-64.4c-8.7 0-15.8-7.1-15.8-15.8v-.4c0-8.7 7.1-15.8 15.8-15.8h64.4c8.7 0 15.8 7.1 15.8 15.8v.4zM351 240c0 9-7.1 16-15.8 16H174.8c-8.7 0-15.8-7.1-15.8-15.8v-.3c0-8.7 7.1-15.9 15.8-15.9h160.3c8.7 0 15.9 7.2 15.9 15.9v.1zM351 304c0 9-7.1 16-15.8 16H174.8c-8.7 0-15.8-7.1-15.8-15.8v-.3c0-8.7 7.1-15.9 15.8-15.9h160.3c8.7 0 15.9 7.2 15.9 16z"></path></svg>
                        </span>
                        <span class="d-inline-block">
                            <p class="text-semibold text-uppercase">Dirección de facturación</p>
                            <p><span class="text-muted">@Model.DireccionFacturacion</span></p>
                            <p>@Model.DistritoFacturacion</p>
                            <p>@Model.ProvinciaFacturacion</p>
                        </span>
                    </div>
                    <div class="popovercontainer address-group ember-view mt-2">
                        <span class="d-inline-block left-right-space align-top">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" class="icon text-overdue "><path d="M487.3 246.6l-77.5-89c-3-3.5-7.4-5.5-12.1-5.5h-65.9V60.5c0-8.8-7.2-16-16-16H36.7c-8.8 0-16 7.2-16 16v329.6c0 8.8 7.2 16 16 16h34c.4 33.9 28 61.3 62 61.3 33.9 0 61.6-27.4 62-61.3h138.9c.4 33.9 28 61.3 62 61.3 33.9 0 61.6-27.4 62-61.3h17.7c8.8 0 16-7.2 16-16v-133c0-3.9-1.4-7.6-4-10.5zM54.7 76.5h243.1c1.1 0 2 .9 2 2v293.6c0 1.1-.9 2-2 2H54.7c-1.1 0-2-.9-2-2V78.5c0-1.1.9-2 2-2zm78 359c-16.3 0-29.6-13.1-30-29.3h59.9c-.3 16.2-13.6 29.3-29.9 29.3zm262.9 0c-16.3 0-29.6-13.1-30-29.3h59.9c-.3 16.2-13.6 29.3-29.9 29.3zm61.7-61.3H333.9c-1.1 0-2-.9-2-2V184.1h57.7c.6 0 1.1.3 1.5.7l67.7 77.8c.3.4.5.8.5 1.3v108.3c0 1.1-.9 2-2 2z"></path><path d="M376.5 216.5c-.4-.4-.9-.7-1.5-.7h-10.4c-1.1 0-2 .9-2 2v43c0 1.1.9 2 2 2h49c1.7 0 2.6-2 1.5-3.3l-38.6-43z"></path></svg>
                        </span>
                        <span class="d-inline-block">
                            <p class="text-semibold text-uppercase">Dirección de envío</p>
                            <p><span class="text-muted">@Model.DireccionEnvio</span></p>
                            <p>@Model.DistritoEnvio</p>
                            <p>@Model.ProvinciaEnvio</p>
                        </span>
                    </div>
                </div>

            </div>
            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                <div style="padding:20px 10px;">
                    @foreach (var c in Model.Contactos)
                    {
                        <div id="616415000000046099" class="person row">
                            <div class="col-md-2">
                                <img src="@Url.Content("~/Content/img/5973cfe4f549038fd9ef21f5260f1afe.png")" class="cp-pic">
                            </div>
                            <div class="col-md-8">
                                <strong>@c.Cuenta</strong>
                                <svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" x="0" y="0" viewBox="0 0 512 512" xml:space="preserve" class="icon icon-xs"><path d="M512 195.6l-171.6-34.7L256 0l-84.4 160.9L0 195.6l119.5 134.1L97.8 512 256 434l158.2 78-21.7-182.3z"></path></svg>
                                <br> @(c.Email ?? "Correo no ingresado")<br>
                                @if (c.PortalHabilitado)
                                {
                                    <small class="text-success">@c.PortalHabilitadoStr</small>
                                }
                                else
                                {
                                    <small class="text-danger">@c.PortalHabilitadoStr</small>
                                }
                            </div>
                        </div>
                    }
                </div>
            </div>
            <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
                <section class="cd-timeline js-cd-timeline">
                    @foreach (var a in Model.Actividades)
                    {
                        <div class="cd-timeline__container">
                            <div class="cd-timeline__block js-cd-block">
                                <div class="cd-timeline__img cd-timeline__img--picture js-cd-img">
                                    <svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" x="0" y="0" viewBox="0 0 512 512" xml:space="preserve" class="icon-comment icon icon-xs text-bottom "><path d="M256.7.2C116.9.2 3.1 115 3.1 255.9 3.1 323.5 29.2 385 72 430.8c-13.5 17.1-45.6 56.7-59.8 63.7-27.6 13.7 9.6 21.3 52.4 12.5 23.1-4.8 47.8-14.2 64.9-30.1 37.5 22 81 34.7 127.3 34.7 139.9 0 253.7-114.8 253.7-255.7S396.6.2 256.7.2zm0 471.5c-118 0-214.1-96.9-214.1-215.8S138.7 40.1 256.7 40.1 470.8 137 470.8 255.9s-96.1 215.8-214.1 215.8z"></path><path d="M115.4 220.7h279.8c10.9 0 19.7-8.9 19.6-19.9 0-11-8.8-19.9-19.7-19.9H115.4c-10.9 0-19.7 8.9-19.7 19.9 0 11 8.8 19.9 19.7 19.9zM395.2 291.2H115.4c-10.9 0-19.7 8.9-19.7 19.9 0 11 8.8 19.9 19.7 19.9h279.8c10.9 0 19.6-8.9 19.6-19.9.1-11-8.7-19.9-19.6-19.9z"></path></svg>
                                </div>
                                <div class="cd-timeline__content js-cd-content">
                                    <div class="arrow"></div>
                                    <p class="text-muted">
                                        <small>@a.Fecha</small>
                                    </p>
                                    <p class="text-regular">
                                        @a.Titulo.DescripcionAtributo()
                                    </p>
                                    <div class="text-muted">
                                        @a.Detalle
                                        <strong class="text-lowercase"> por @a.Usuario</strong>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </section>
            </div>
        </div>
    </div>
</div>